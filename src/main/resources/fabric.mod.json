{
	"schemaVersion": 1,
	"id": "rustcraft",
	"version": "${version}",
	"name": "RustCraft",
	"description": "Implementation of Rust to Minecraft",
	"authors": [
		"Rafita2840"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/Rafita2840/RustCraft"
